{
  "name": "animesh-cli",
  "version": "0.0.1",
  "description": "CLI wrapper for Animesh Portfolio components.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start:dev": "node dist/index.js",
    "publish:release": "pnpm build && pnpm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "animesh",
    "cli",
    "portfolio"
  ],
  "author": "Animesh Singh Gosain <animeshlego5@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/animeshlego5/animesh-portfolio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/animeshlego5/animesh-portfolio.git",
    "directory": "packages/animesh-cli"
  },
  "bugs": {
    "url": "https://github.com/animeshlego5/animesh-portfolio/issues"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "packageManager": "pnpm@10.10.0"
}